{
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "manifest_version": 2,
  "name": "应用公社",
  "version": "1.0",
  "description": "我的第一个Chrome应用例子",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "permissions": [
    "http://localhost/*",
    "http://localhost/shell*",
    "system.cpu",
    "system.memory",
    "webview"
  ]
}
